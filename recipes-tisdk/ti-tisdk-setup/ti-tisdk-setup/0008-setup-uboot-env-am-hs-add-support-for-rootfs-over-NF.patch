+		do_expect "\"$prompt\"" "send \"saveenv\"" $cwd/setupBoard.minicom
+		do_expect "\"$prompt\"" "send \"boot\"" $cwd/setupBoard.minicom
 
-do_expect "\"$prompt\"" "send \"boot\"" $cwd/setupBoard.minicom
+	fi
+fi
